<table style="width:100%">
    <tr>
        <th>Title</th>
        <th>Price</th>
        <th>Release Date</th>
        <th>Label</th>
        <th>Main Genres</th>
    </tr>
    <% @products.each do |product| %>
        <tr>
            <td><%= product.title %></td>
            <td><%= product.price %></td>
            <td><%= product.release_date %></td>
            <td><%= product.label %></td>
        </tr>
    <% end %>
</table>